te.detailData.noticesTableOptions;\n      },\n      set(newOptions) {\n        this.$store.dispatch(\"detailData/setNoticesTableOptions\", newOptions);\n      },\n    },\n    loading() {\n      return this.data.loading;\n    },\n  },\n  watch: {\n    options: {\n      handler() {\n        this.getDataFromApi();\n      },\n      deep: true,\n    },\n  },\n\n  methods: {\n    getDataFromApi() {\n      this.$store\n        .dispatch(\"detailData/getNotices\", this.options)\n        .then(() => {});\n    },\n  },\n};\n</script>\n\n<style scoped>\n.v-card {\n  overflow-y: auto;\n  overflow-x: auto;\n}\n</style>"],"sourceRoot":"src/components/chartComponents"}]}